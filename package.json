        "title_options": null,
        "options": null
      },
      "form": {
        "css": "modal-lg",
        "inputs": [
          {
            "type": "input",
            "placeholder": "Conn1",
            "title": {
              "es": "Sesión:",
              "en": "Session:"
            },
            "description": {
              "es": "Nombre que se desea poner a la sesión",
              "en": "Name for identify the session"
            },
            "id": "session",
            "css": "col-md-6"
          },
          {
            "type": "input",
            "placeholder": "sys",
            "title": {
              "es": "Usuario:",
              "en": "User:"
            },
            "description": {
              "es": "Nombre de usuario de la base de datos",
              "en": "Name of the user to connect to the data base"
            },
            "id": "user",
            "css": "col-md-6"
          },
          {
            "type": "input",
            "placeholder": "password",
            "title": {
              "es": "Contraseña:",
              "en": "Password:"
            },
            "description": {
              "es": "Password del usuario previamente usado",
              "en": "Password from the user previously used"
            },
            "id": "password",
            "css": "col-md-4"
          },
          {
            "type": "input",
            "placeholder": "Firebird.host.com/ORCL",
            "title": {
              "es": "DSN Hostname de Conexion:",
              "en": "DSN Hostname:"
            },
            "description": {
              "es": "Hostname asignado en el DSN",
              "en": "Hostname from the DSN"
            },
            "id": "dsnHostname",
            "css": "col-md-4"
          },
          {
            "type": "input",
            "placeholder": "1521",
            "title": {
              "es": "DSN Puerto de Conexion:",
              "en": "DSN Port:"
            },
            "description": {
              "es": "Puerto asignado en el DSN",
              "en": "Port from the DSN"
            },
            "id": "dsnPort",
            "css": "col-md-4"
          },
          {
            "type": "input",
            "placeholder": "pdborcl / pdborcl.example.com",
            "title": {
              "es": "DSN SID / Service name de Conexion:",
              "en": "DSN SID / Service name:"
            },
            "description": {
              "es": "SID o nombre de servicio asignado en el DSN",
              "en": "SID or service name from the DSN"
            },
            "id": "dsnSID",
            "css": "col-md-4"
          },
          {
            "type": "input",
            "placeholder": "var",
            "title": {
              "es": "Asignar a variable:",
              "en": "Assign to variable:"
            },
            "description": {
              "es": "Variable donde se desea guardar el resultado.",
              "en": "Varialbe where to store the result."
            },
            "help": {
              "es": "Nombre de variable sin {}",
              "en": "Variable name without {}"
            },
            "id": "result",
            "css": "col-md-4"
          }
        ]
      },
