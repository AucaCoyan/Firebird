        "description": "Cierra una conexión de oracle por sesión",
        "title_options": null,
        "options": null
      },
      "form": {
        "css": "modal-md",
        "inputs": [
          {
            "type": "input",
            "placeholder": "Conn1",
            "title": {
              "es": "Sesión:",
              "en": "Session:"
            },
            "description": {
              "es": "Nombre de la sesion previamente iniciada que se desea cerrar",
              "en": "Name of the session previously started that you want to close"
            },
            "id": "session",
            "css": "col-md-12"
          }
        ]
      },
      "video_youtube": "",
