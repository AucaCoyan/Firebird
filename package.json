      "form": {
        "css": "modal-lg",
        "inputs": [
          {
            "type": "input",
            "placeholder": "SELECT * from table",
            "title": {
              "es": "Query:",
              "en": "Query:"
            },
            "description": {
              "es": "Query que se desea ejecutar en la base de datos",
              "en": "Query to execute in the database"
            },
            "id": "query",
            "css": "col-md-12"
          },
          {
            "type": "input",
            "placeholder": "Conn1",
            "title": {
              "es": "Sesión:",
              "en": "Session:"
            },
            "description": {
              "es": "Nombre de la sesion previamente iniciada",
              "en": "Name of the session previously started"
            },
            "id": "session",
            "css": "col-md-6"
          },
          {
            "type": "input",
            "placeholder": "var",
            "title": {
              "es": "Asignar resultado a variable:",
              "en": "Set result to var:"
            },
            "description": {
              "es": "Variable donde se desea guardar el resultado.",
              "en": "Varialbe where to store the result."
            },
            "help": {
              "es": "Nombre de variable sin {}",
              "en": "Variable name without {}"
            },
            "id": "result",
            "css": "col-md-6"
          }
        ]
